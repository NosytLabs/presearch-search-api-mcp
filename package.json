  "bugs": {
    "url": "https://github.com/trycatchjames/presearch-mcp-server/issues"
  },
  "homepage": "https://presearch.io",
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE",
    ".env.example"
  ],
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.15.1",
    "axios": "^1.7.2",
