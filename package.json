{
  "name": "presearch-mcp-server",
  "version": "2.1.5",
  "description": "MCP server for Presearch decentralized search engine",
  "type": "module",
  "main": "src/index.js",
  "bin": {
    "presearch-mcp-server": "src/index.js"
  },
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "node --test tests/*.test.js",
    "test:tools": "node integration-tests/test-tools.js",
    "test:search": "node integration-tests/test-search.js",
    "lint": "eslint src/ --fix",
    "format": "prettier --write \"**/*.{js,json,md}\""
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.6.0",
    "axios": "^1.6.0",
    "cheerio": "^1.0.0-rc.12",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "puppeteer": "^21.5.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.1.0"
  },
  "keywords": [
    "mcp",
    "presearch",
    "search",
    "ai",
    "llm",
    "claude",
    "decentralized"
  ],
  "author": "Presearch Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NosytLabs/presearch-search-api-mcp.git"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
